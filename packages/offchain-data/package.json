{
  "name": "@palladco/offchain-data",
  "description": "Off-chain data fetching services.",
  "type": "module",
  "version": "1.0.0",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "bunx --bun tsc --noEmit && bunx --bun tsup",
    "dev": "bunx --bun tsup --watch",
    "cleanup": "bunx --bun rimraf node_modules dist .turbo"
  },
  "dependencies": {
    "dayjs": "1.11.12",
    "swr": "2.2.5"
  },
  "devDependencies": {
    "@palladco/common": "workspace:*"
  }
}
