{
  "name": "@palladxyz/providers",
  "version": "0.0.1",
  "description": "Proivder Package SDK for Pallad.",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
