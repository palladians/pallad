    "graphql": "16.9.0",
    "graphql-request": "7.1.0",
    "json-bigint": "1.0.0",
    "mina-signer": "3.0.7",
    "subscriptions-transport-ws": "0.11.0",
    "viem": "2.18.4",
    "ws": "8.18.0"
  },
  "devDependencies": {
    "@palladxyz/common": "workspace:*",
    "@types/events": "3.0.3",
    "@types/json-bigint": "1.0.4",
